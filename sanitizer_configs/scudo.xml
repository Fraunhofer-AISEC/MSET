<sanitizer>
    <name>Scudo</name>
    <setup>
        <compile_cmd>clang -Wl,-T,../../sanitizers/after_text.ld -fsanitize=scudo $SOURCE_FILE -o $GENERATED_BINARY</compile_cmd>
    </setup>
    <setup_baseline>
        <compile_cmd>clang -Wl,-T,../../sanitizers/after_text.ld $SOURCE_FILE -o $GENERATED_BINARY</compile_cmd>
    </setup_baseline>
    <run>$GENERATED_BINARY</run>
    <run_baseline>$GENERATED_BINARY</run_baseline>
    <bug_detected_exit_values>
        <value>6</value>
    </bug_detected_exit_values>
</sanitizer>
