<sanitizer>
    <name>SoftBound+CETS</name>
    <setup_baseline>
        <compile_cmd>../../sanitizers/softbound/build/bin/clang -Wl,-T,../../sanitizers/after_text.ld $SOURCE_FILE -o $GENERATED_BINARY</compile_cmd>
    </setup_baseline>
    <run_baseline>$GENERATED_BINARY</run_baseline>
    <setup>
        <compile_cmd>../../sanitizers/softbound/build/bin/clang -Wl,-T,../../sanitizers/after_text.ld -lm -g -fsanitize=softboundcets $SOURCE_FILE -o $GENERATED_BINARY</compile_cmd>
    </setup>
    <run>$GENERATED_BINARY</run>
    <bug_detected_exit_values>
        <value>6</value>
    </bug_detected_exit_values>
</sanitizer>
